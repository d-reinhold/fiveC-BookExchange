<h1>Sign In</h1>
<%= form_tag sessions_path do %>
  <div class="field">
    <%= text_field_tag :email, params[:email] %><%= label_tag :email %>
  </div>
  <div class="field">
    <%= password_field_tag :password %><%= label_tag :password %>
  </div>
  <div class="actions"><%= submit_tag "Log in" %></div>
<% end %>
