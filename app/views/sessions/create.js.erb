<% unless @current_user.nil? %>
  $("#sell-form-final").fadeOut("slow", function(){
    $("#new_listing").submit();
  });
<% else %>
  $("p#password").parent().css("border", "1px solid #FF6161") 
  $("p#password").parent().css("background-color", "#FFA8A8")
  $("p#password").text("Sorry, that password isn't correct!")
  $("p#password-signin").parent().css("border", "1px solid #FF6161") 
  $("p#password-signin").parent().css("background-color", "#FFA8A8")
  $("p#password-signin").text("Sorry, that password isn't correct!")
<% end %>